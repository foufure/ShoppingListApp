@foreach (KeyValuePair<string,string> supportedCulture in ApplicationCultures.SupportedCultures)
{ 
    @Html.ActionLink(supportedCulture.Key, "SaveCultureChosenByUser", "UserCulture", new { userCulture = supportedCulture.Value, returnUrl = Request.Url.PathAndQuery }, new { @class = "btn btn-primary hidden-print" })
}